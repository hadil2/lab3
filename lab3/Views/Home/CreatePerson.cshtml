@{
    Layout = "_Layout";
    ViewData["Title"] = "Create Person";
}

<form asp-controller="Home" asp-action="DisplayPerson" method="post">
    <table>
        <tr>
            <td>
                First Name:
                <input type="text" name="first"/>
            </td>
        
        </tr>

        <tr>
            <td>
                Last Name: 
                <input type="text" name="lastName"/>
            </td>
        
        </tr>

        <tr>
            <td>
                Age:
                <input type="text" name="age"/>
            </td>
        
        </tr>

        <tr>
            <td>
                Email Address:
                <input type="text" name="email"/>
            </td>
        
        </tr>

        <tr>
            <td>
                Password:
                <input type="password" name="password"/>
            </td>
        
        </tr>

        <tr>
            <td>
                Description:
                <input type="text" name="description"/>
            </td>
        
        </tr>

        <tr>
            <td>
                <button type="submit" value="Create!" name="createButton"> Create! </button>
            </td>
        </tr>
    </table>

    
    

</form>
